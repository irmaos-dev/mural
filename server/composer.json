{
    "name": "f1amy/laravel-realworld-example-app",
    "type": "project",
    "description": "Laravel framework RealWorld backend implementation.",
    "keywords": ["laravel", "jwt", "crud", "demo-app", "realworld", "rest-api"],
    "homepage": "https://github.com/f1amy/laravel-realworld-example-app",
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "ext-json": "*",
        "ext-pdo": "*",
        "darkaonline/l5-swagger": "^8.3",
        "doctrine/dbal": "^3.0",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^11.0",
        "laravel/passport": "^12.0",
        "laravel/sanctum": "^4.0",
        "laravel/socialite": "^5.16",
        "laravel/tinker": "^2.7",
        "spatie/laravel-activitylog": "^4.9",
        "spatie/laravel-permission": "^6.9"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.1",
        "brianium/paratest": "^7.0",
        "fakerphp/faker": "^1.9.1",
        "larastan/larastan": "^2.7.0",
        "laravel/pint": "^1.17",
        "laravel/sail": "^1.31",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^8.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^1.4",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^10.0",
        "roave/security-advisories": "dev-latest",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "[ $COMPOSER_DEV_MODE -eq 0 ] || $PHP_BINARY artisan ide-helper:generate -WHM",
            "[ $COMPOSER_DEV_MODE -eq 0 ] || $PHP_BINARY artisan ide-helper:meta"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "8.3"
        },
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
